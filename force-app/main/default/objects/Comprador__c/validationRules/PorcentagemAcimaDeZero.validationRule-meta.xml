<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PorcentagemAcimaDeZero</fullName>
    <active>true</active>
    <errorConditionFormula>AND(PorcentagemDeParticipacao__c &lt;= 0)</errorConditionFormula>
    <errorDisplayField>PorcentagemDeParticipacao__c</errorDisplayField>
    <errorMessage>A Porcentagem tem que ser acima de 0%</errorMessage>
</ValidationRule>
