<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MotivoEmBranco</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISPICKVAL(StatusVisita__c, &apos;Cancelado&apos;),
    ISBLANK(TEXT(MotivoCompromissoNaoRealizado__c))
)</errorConditionFormula>
    <errorMessage>Preencha o Motivo</errorMessage>
</ValidationRule>
