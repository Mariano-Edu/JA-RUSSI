<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DataFimNaoPodeSerAnteriorDataInicio</fullName>
    <active>true</active>
    <errorConditionFormula>VigenciaFim__c &lt; VigenciaInicio__c</errorConditionFormula>
    <errorDisplayField>VigenciaFim__c</errorDisplayField>
    <errorMessage>A Data de Fim da Vigência não pode ser anterior à Data de Início da Vigência.</errorMessage>
</ValidationRule>
